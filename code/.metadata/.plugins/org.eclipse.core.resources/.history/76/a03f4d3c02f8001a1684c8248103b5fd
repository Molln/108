package com.molln.q108;
import java.io.InputStream;

public class Question001 {

	public static void main(String[] args) throws Exception{
		int length = 0 ;
		InputStream in = System.in;
		char c = (char)in.read();
		while(c != '\n') {
			if(c == ' ') {
				length = 0;
			}else {
				c++;
			}
			c = (char)in.read();
		}
		System.out.println(c);
	}

}
