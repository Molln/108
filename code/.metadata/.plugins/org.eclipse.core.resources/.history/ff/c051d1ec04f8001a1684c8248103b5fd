package com.molln.q108;

import java.io.InputStream;

public class Question001 {
	
	public static void main(String[] args) throws Exception {
		int times=0;
        InputStream in=System.in;
        int c= in.read();
        while(c != -1) {
        	c = in.read();
        	System.out.println(c);
        }
	}

}
